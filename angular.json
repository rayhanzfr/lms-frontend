{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "lms": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/lms",
      "sourceRoot": "projects/lms/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/lms",
            "index": "projects/lms/src/index.html",
            "main": "projects/lms/src/main.ts",
            "polyfills": "projects/lms/src/polyfills.ts",
            "tsConfig": "projects/lms/tsconfig.app.json",
            "assets": [
              "projects/lms/src/favicon.ico",
              "projects/lms/src/assets"
            ],
            "styles": [
              "projects/lms/src/styles.css",
              "projects/lms/src/template/plugins/fontawesome-free/css/all.css",
              "projects/lms/src/template/plugins/overlayScrollbars/css/OverlayScrollbars.css",
              "projects/lms/src/template/dist/css/adminlte.css"
            ],
            "scripts": [
              "projects/lms/src/template/plugins/jquery/jquery.js",
              "projects/lms/src/template/plugins/bootstrap/js/bootstrap.bundle.js",
              "projects/lms/src/template/plugins/overlayScrollbars/js/jquery.overlayScrollbars.js",
              "projects/lms/src/template/dist/js/adminlte.js",
              "projects/lms/src/template/dist/js/demo.js",
              "projects/lms/src/template/plugins/jquery-mousewheel/jquery.mousewheel.js",
              "projects/lms/src/template/plugins/raphael/raphael.js",
              "projects/lms/src/template/plugins/jquery-mapael/jquery.mapael.js",
              "projects/lms/src/template/plugins/chart.js/Chart.js",
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/lms/src/environments/environment.ts",
                  "with": "projects/lms/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "lms:build:production"
            },
            "development": {
              "browserTarget": "lms:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "lms:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/lms/src/test.ts",
            "polyfills": "projects/lms/src/polyfills.ts",
            "tsConfig": "projects/lms/tsconfig.spec.json",
            "karmaConfig": "projects/lms/karma.conf.js",
            "assets": [
              "projects/lms/src/favicon.ico",
              "projects/lms/src/assets"
            ],
            "styles": [
              "projects/lms/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "core": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/core",
      "sourceRoot": "projects/core/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/core",
            "index": "projects/core/src/index.html",
            "main": "projects/core/src/main.ts",
            "polyfills": "projects/core/src/polyfills.ts",
            "tsConfig": "projects/core/tsconfig.app.json",
            "assets": [
              "projects/core/src/favicon.ico",
              "projects/core/src/assets"
            ],
            "styles": [
              "projects/core/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/core/src/environments/environment.ts",
                  "with": "projects/core/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "core:build:production"
            },
            "development": {
              "browserTarget": "core:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "core:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/core/src/test.ts",
            "polyfills": "projects/core/src/polyfills.ts",
            "tsConfig": "projects/core/tsconfig.spec.json",
            "karmaConfig": "projects/core/karma.conf.js",
            "assets": [
              "projects/core/src/favicon.ico",
              "projects/core/src/assets"
            ],
            "styles": [
              "projects/core/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "lms"
}
